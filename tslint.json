{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "object-literal-sort-keys": false,
        "interface-name": false,
        "member-access": [true, "no-public"],
        "ordered-imports": false,
        "max-classes-per-file": ["error", 5],
        "prefer-const": [false],
        "max-line-length": [true, 150]
    },
    "rulesDirectory": []
}
